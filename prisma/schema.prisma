/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model Commits {
  id              BigInt    @id(map: "Commit_pkey") @default(autoincrement())
  cid             String
  author_email    String?
  author_name     String?
  authored_date   DateTime? @db.Timestamptz(6)
  committed_date  DateTime? @db.Timestamptz(6)
  committer_email String?
  committer_name  String?
  created_at      DateTime? @db.Timestamptz(6)
  title           String?
  message         String?
  project_id      BigInt?
  short_id        String?
  stats_additions BigInt?
  stats_deletions BigInt?
  stats_total     BigInt?
  status          String?
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model Projects {
  id          Int       @id
  pid         Int
  name        String?   @db.VarChar(500)
  description String
  createdAt   DateTime? @db.Timestamptz(6)
  updateAt    DateTime? @db.Timestamptz(6)
}

model idols {
  id                  BigInt    @id @unique @default(autoincrement())
  realName            String?
  englishName         String?
  nickName            String?
  avatar              String?
  height              Int?
  birthday            String?
  constellation       String?
  hobby               String?
  speciality          String?
  team                String?
  interactWeibo       String?
  interactXiaohongshu String?
  videoNames          String?
  videoLinks          String?
  remark1             String?
  remark2             String?
  createdAt           DateTime? @db.Timestamptz(6)
  updatedAt           DateTime? @db.Timestamptz(6)
}
